( kan_0183 " ಮುಂದೆ ಕರ್ನಾಟಕ ವಿಶ್ವವಿದ್ಯಾಲಯ ದಲ್ಲಿ ಇತಿಹಾಸದಲ್ಲಿ ಎಂ.ಎ. ಪದವಿ ಗಳಿಸಿದರು ")
( kan_0184 " ಕಾಂಡ ಕಲ್ಕತ್ತಾ ಪಶ್ಚಿಮ ಬಂಗಾಳ ಭಾರತ ")
( kan_0185 " ಪ್ರಹ್ಲಾದ ರಾವ್ ರವರು ರಚಿಸಿರುವ ಡಾ.ರಾಜ್‌ಕುಮಾರ್ ಆತ್ಮಚರಿತ್ರೆಗಾಗಿ ಈ ಪುಟವನ್ನು ನೋಡಿ ")
( kan_0186 " ಇಟಲಿ ದಕ್ಷಿಣ ಯುರೋಪ್ ನ ಒಂದು ದೇಶ ")
( kan_0187 " ಭಾರತ ದ ಪ್ರಸಿದ್ಧ ಸಿತಾರ್ ವಾದಕ ಪಂಡಿತ್ ರವಿ ಶಂಕರ್ ")
( kan_0188 " ಇವರು ವ್ಯಾಸಂಗ ಮಾಡಿದ್ದು ಬೆಂಗಳೂರು ವಿಶ್ವವಿದ್ಯಾಲಯದಲ್ಲಿ ")
( kan_0189 " ಅಮೇರಿಕ ದೇಶದ ಪ್ರಸಿದ್ಧ ನಿರ್ದೇಶಕರು ನಿರ್ಮಾಪಕರು ಲೇಖಕರು ")
( kan_0190 " ಸ್ವಾತಂತ್ರ್ಯ ಹೋರಾಟ ಹೆಸರಿನ ಲೇಖನವನ್ನು ನೀವು ಪ್ರಾರಂಭಿಸಬಹುದು ")
( kan_0191 " ಬ್ರೆಜಿಲ್ ದೇಶದ ಏಕೈಕ ಅಧಿಕೃತ ಭಾಷೆ ಪೋರ್ಚುಗೀಸ್ ")
( kan_0192 " ಸಾಗರ ಶಿವಮೊಗ್ಗ ಜಿಲ್ಲೆಯ ಒಂದು ಪಟ್ಟಣ ಮತ್ತು ಅದೇ ಹೆಸರಿನ ತಾಲೂಕಿನ ಆಡಳಿತ ಕೇಂದ್ರ ")
( kan_0193 " ಕರ್ನಾಟಕ ಸಂಘ ಶಿವಮೊಗ್ಗ ದಿಂದ ಇಂದಿರಾ ಪುರಸ್ಕಾರ ")
( kan_0194 " ಜ್ಞಾನಪೀಠ ಪ್ರಶಸ್ತಿ ಪಡೆದ ಕನ್ನಡ ಸಾಹಿತಿಗಳ ಪಟ್ಟಿ ಹೀಗಿದೆ ")
( kan_0195 " ದಯಮಾಡಿ ಅವರ ಹೆಸರುಗಳನ್ನು ಸೇರಿಸಿ ಅಥವಾ ಅವರ ಬಗ್ಗೆ ಲೇಖನವನ್ನು ಬರೆದು ಈ ವರ್ಗಕ್ಕೆ ಸೇರಿಸಿ ")
( kan_0196 " ಚದುರಂಗ ಕನ್ನಡ ಚಲನಚಿತ್ರದ ಬಗ್ಗೆ ಮಾಹಿತಿಗೆ ಈ ಲೇಖನವನ್ನು ಓದಿ ")
( kan_0197 " ಗಿನಿ ಅಧಿಕೃತವಾಗಿ ಗಿನಿ ಗಣರಾಜ್ಯ ಪಶ್ಚಿಮ ಆಫ್ರಿಕಾ ದ ಒಂದು ರಾಷ್ಟ್ರ ")
( kan_0198 " ಬೊಂಬಾಯಿ ನಗರದಲ್ಲಿ ಉಚ್ಚ ಶಿಕ್ಷಣವನ್ನು ಪ್ರಥಮವಾಗಿ ಪ್ರಾರಂಭಿಸಿದರು ")
( kan_0199 " ಕರ್ನಾಟಕ ವಿಶ್ವವಿದ್ಯಾಲಯವು ಇವರಿಗೆ ಗೌರವ ಡಾಕ್ಟರೇಟ್ ನೀಡಿದೆ ")
( kan_0200 " ಅಮೇರಿಕ ದೇಶದ ಸಂವಿಧಾನ ಪ್ರಪಂಚದ ಅತ್ಯಂತ ಹಳೆಯ ಆಧುನಿಕ ಲಿಖಿತ ಸಂವಿಧಾನ ")
( kan_0201 " ಕನ್ನಡ ಚಿತ್ರರಂಗ ದ ಒಬ್ಬ ಚಲನಚಿತ್ರ ನಿರ್ದೇಶಕ ಮತ್ತು ನಟ ")
( kan_0202 " ಈ ಪ್ರದೇಶದ ಪ್ರಧಾನ ಭಾಷೆ ಮತ್ತು ಅಧಿಕೃತ ಭಾಷೆ ಕನ್ನಡ ")
( kan_0203 " ಕಡೂರು ಇದು ಚಿಕ್ಕಮಗಳೂರು ಜಿಲ್ಲೆಯ ಪ್ರಮುಖ ಪಟ್ಟಣ ")
( kan_0204 " ಇದು ಪ್ರಪಂಚದಲ್ಲಿಯೇ ಅತಿ ದೊಡ್ಡ ಕ್ರೈಸ್ತ ದೇವಾಲಯ ")
( kan_0205 " ಹಿಂದೂ ಧರ್ಮದ ಚಾಂದ್ರಮಾನ ಪಂಚಾಂಗದ ಎರಡನೇ ಮಾಸ ")
( kan_0206 " ಸರ್ಕಾರಿ ಕೆಲಸದಲ್ಲಿ ಅವರ ಮನಸ್ಸು ತುಂಬಾ ದಿನ ಇರಲಿಲ್ಲ ")
( kan_0207 " ಮೂಲ ಮಧ್ಯ ಅಮೇರಿಕ ಹಾಗೂ ದಕ್ಷಿಣ ಅಮೇರಿಕ ")
( kan_0208 " ಸಿನಿಮಾ ರಂಗದ ಶ್ರೇಷ್ಠ ಸಾಧನೆಗಾಗಿ ಪ್ರತಿಷ್ಠಿತ ಡಾ.ರಾಜ್‌ಕುಮಾರ್ ಪ್ರಶಸ್ತಿ ")
( kan_0209 " ದಕ್ಷಿಣ ಏಷ್ಯಾ ದಲ್ಲಿನ ಒಂದು ದೇಶ ")
( kan_0210 " ಹೂವು ಮತ್ತು ಎಲೆ ಕಲ್ಕತ್ತಾ ಪಶ್ಚಿಮ ಬಂಗಾಳ ಭಾರತ ")
( kan_0211 " ಈ ಲೇಖನ ಶಿವಮೊಗ್ಗ ಜಿಲ್ಲೆಯಲ್ಲಿರುವ ಊರಿನ ಬಗ್ಗೆ ")
( kan_0212 " ಮಹಾಭಾರತ ದಲ್ಲಿ ಬರುವ ಪಾತ್ರಗಳ ಕುರಿತಾದ ಲೇಖನ ವರ್ಗ ")
( kan_0213 " ಸ್ವಾತಂತ್ರ್ಯ ಹೋರಾಟ ಪದವನ್ನು ಬೇರೆ ಲೇಖನಗಳಲ್ಲಿ ಹುಡುಕಿ ")
( kan_0214 " ಇಂಗ್ಲಿಷ್ ಮತ್ತು ಫ್ರೆಂಚ್ ಭಾಷೆ ಈ ಸಂಘದ ಅಧಿಕೃತ ಭಾಷೆಗಳು ")
( kan_0215 " ಮೈಸೂರು ವಿಶ್ವವಿದ್ಯಾಲಯದಲ್ಲಿ ಇಂಗ್ಲೀಷ್ ಸಾಹಿತ್ಯದಲ್ಲಿ ಎಂ.ಎ. ಓದು ಮುಗಿಸಿದರು ")
( kan_0216 " ಒಂದು ಇಲ್ಲಿ ಭಾರಿ ಗಣಪತಿಯ ದೇವಾಲಯವಿದೆ ")
( kan_0217 " ಬ್ರೆಜಿಲ್ ದೇಶದ ಅತಿ ದೊಡ್ಡ ನಗರ ")
( kan_0218 " ಪ್ರವಾಸೋದ್ಯಮ ದೇಶದ ಪ್ರಮುಖ ಆರ್ಥಿಕ ಚಟುವಟಿಕೆ ")
( kan_0219 " ವಿಶ್ವ ಕನ್ನಡ ಚಲನಚಿತ್ರದ ಬಗ್ಗೆ ಮಾಹಿತಿಗೆ ಈ ಲೇಖನವನ್ನು ಓದಿ ")
( kan_0220 " ಮರಾಠಿ ನಾಟಕಗಳ ಮೂಲಕ ತಮ್ಮ ವೃತ್ತಿ ಜೀವನವನ್ನು ಪ್ರಾರಂಭ ಮಾಡಿದರು ")
( kan_0221 " ಇದರ ಬಗ್ಗೆ ಏನಾದರೂ ಪ್ರಶ್ನೆಗಳಿದ್ದರೆ ನನ್ನ ಚರ್ಚೆ ಪುಟದಲ್ಲಿ ಕೇಳಿ ")
( kan_0222 " ಮಾಹಿತಿ ತಂತ್ರಜ್ಞಾನ ವಿಶ್ವಕೋಶದ ಸಂಪಾದಕ ಮಂಡಳಿಯ ಸದಸ್ಯರಾಗಿ ಕೆಲಸ ಮಾಡುತ್ತಿದ್ದಾರೆ ")
( kan_0223 " ಉತ್ತರ ಯುರೋಪ್ ನ ಒಂದು ದ್ವೀಪರಾಷ್ಟ್ರ ")
( kan_0224 " ಕರ್ನಾಟಕ ವಿಶ್ವವಿದ್ಯಾಲಯ ವು ಇವರಿಗೆ ಗೌರವ ಡಾಕ್ಟರೇಟ್ ನೀಡಿದೆ ")
( kan_0225 " ಇದು ಪುಟ್ಟಣ್ಣ ಕಣಗಾಲ್ ನಿರ್ದೇಶನದ ಕಾಲೇಜು ರಂಗ ಚಲನಚಿತ್ರದ ಮಾಹಿತಿ ಪುಟ ")
( kan_0226 " ಕೋಲಾರ ಜಿಲ್ಲೆಯ ಪ್ರಮುಖ ಭಾಷೆ ಕನ್ನಡ ")
( kan_0227 " ಇದು ವಿಶ್ವದ ಅತಿ ಪುಟ್ಟ ದ್ವೀಪರಾಷ್ಟ್ರ ಹಾಗೂ ಅತಿ ಪುಟ್ಟ ಸ್ವತಂತ್ರ ಗಣರಾಜ್ಯ ")
( kan_0228 " ಕರ್ನಾಟಕ ಸಾಹಿತ್ಯ ಅಕಾಡೆಮಿ ಪುಸ್ತಕ ಬಹುಮಾನ ಪಡೆದ ಕೃತಿ ")
( kan_0229 " ಹಿಂದೂ ಧರ್ಮದ ಸೌರಮಾನ ಪಂಚಾಂಗದ ಎರಡನೆ ಮಾಸ ")
( kan_0230 " ಇದರ ಬಗ್ಗೆ ಅವರಿಗೆ ಯಾವುದೇ ಉತ್ತರ ಬರಲಿಲ್ಲ ")
( kan_0231 " ಕನ್ನಡ ಚಿತ್ರರಂಗ ದ ಒಬ್ಬ ಪ್ರಸಿದ್ದ ನಿರ್ದೇಶಕ ಮತ್ತು ನಿರ್ಮಾಪಕ ")
( kan_0232 " ನಾನು ಹುಟ್ಟಿ ಬೆಳೆದ ಊರು ಬೆಂಗಳೂರು ")
( kan_0233 " ಇದು ಭಾರತದ ಅತಿ ಎತ್ತರದ ಜಲಪಾತ ")
( kan_0234 " ಜರ್ಮನಿ ಪಶ್ಚಿಮ ಯುರೋಪ್ ನ ಒಂದು ದೇಶ ")
( kan_0235 " ಗ್ರೆಗೋರಿಯನ್ ಕ್ಯಾಲೆಂಡರ್ ಪ್ರಪಂಚದಲ್ಲಿ ಅತಿ ಹೆಚ್ಚಾಗಿ ಉಪಯೋಗಿಸಲ್ಪಡುವ ಕ್ಯಾಲೆಂಡರ್ ಪಂಚಾಂಗ ")
( kan_0236 " ಇದರ ಕಥೆ ರಚನೆ ನಿರ್ದೇಶಕ ಮತ್ತು ನಾಯಕ ನಟ ರವಿಚಂದ್ರನ್ ")
( kan_0237 " ಹಲವು ವಿಷಯಗಳ ಮೇಲೆ ಕೆಲಸ ನಡೆಯುತ್ತಿದೆ ")
( kan_0238 " ವಿಕಿಪೀಡಿಯ ಸಮುದಾಯದ ಪುಟಗಳ ವರ್ಗಗಳು ಹಾಗೂ ಲೇಖನಗಳ ಪಟ್ಟಿ ")
( kan_0239 " ಧೂಮಕೇತು ಕನ್ನಡ ಚಲನಚಿತ್ರದ ಬಗ್ಗೆ ಮಾಹಿತಿಗೆ ಈ ಲೇಖನವನ್ನು ನೋಡಿ ")
( kan_0240 " ಹೊಯ್ಸಳ ಕಾಲದ ಶ್ರೀ ಸೋಮೇಶ್ವರ ದೇವಾಲಯ ಇಲ್ಲಿನ ಪ್ರಮುಖ ದೇವಾಲಯಗಳಲ್ಲಿ ಒಂದು ")
( kan_0241 " ಒಟ್ಟು ಹನ್ನೊಂದು ಹಿಂದಿ ಮತ್ತು ಎಂಟು ಮರಾಠಿ ಚಿತ್ರಗಳಿಗೆ ಅವರು ಕಥೆ ಬರೆದಿದ್ದಾರೆ ")
( kan_0242 " ಭಾಷೆ ಹಾಗು ಭಾಷಾ ವಿಜ್ಞಾನದ ಬಗೆಗಿನ ಲೇಖನಗಳು ")
( kan_0243 " ಈ ಸಾಹಿತಿ ದಂಪತಿಗಳ ಏಕೈಕ ಪುತ್ರಿ ಎಮ್.ಎ. ಪದವಿ ಪಡೆದು ")
( kan_0244 " ಈ ಚಿತ್ರದಲ್ಲಿ ಪ್ರತೀ ಘಟನೆ ಪ್ರತೀ ಪಾತ್ರ ಒಂದು ಸಂದೇಶವನ್ನು ಹೊಂದಿದೆ ")
( kan_0245 " ಕನ್ನಡ ವಿಕಿಪೀಡಿಯಕ್ಕೆ ಸಂಬಂಧಿಸಿದ ಚಟುವಟಿಕೆಗಳ ಪಟ್ಟಿ ಇಲ್ಲಿ ನಿಮಗೆ ಓದಲು ಸಿಗುತ್ತದೆ ")
( kan_0246 " ತಮ್ಮ ಶಾಸನಗಳ ಅಧ್ಯಯನದ ಫಲವಾಗಿ ಶಾಸನಗಳಲ್ಲಿ ಕಂಡುಬರುವ ಕವಿಗಳು ಎನ್ನುವ ಗ್ರಂಥವನ್ನು ರಚಿಸಿದ್ದಾರೆ ")
( kan_0247 " ಅನುವಾದ ಸಾಹಿತ್ಯದ ಮೂಲಕ ಡಿ ವಿ ಜಿ ಉತ್ತಮ ಹೆಸರು ಪಡೆದರು ")
( kan_0248 " ಮಾಸ್ತಿ ಅವರಿಗೆ ಸಂದ ಗೌರವ ಪ್ರಶಸ್ತಿಗಳು ಅಪಾರ ")
( kan_0249 " ಈ ಕೃತಿಗೆ ಕೇಂದ್ರ ಸಾಹಿತ್ಯ ಅಕಾಡೆಮಿ ಪ್ರಶಸ್ತಿ ಲಭಿಸಿದೆ ")
( kan_0250 " ದಕ್ಷಿಣ ಭಾರತ ದಲ್ಲಿ ವ್ಯಾಪಕವಾಗಿ ಬೆಳೆಯುತ್ತದೆ ")
( kan_0251 " ಭಾರತ ದ ಕರ್ನಾಟಕ ರಾಜ್ಯದಲ್ಲಿ ಉಪಯೋಗಿಸಲ್ಪಡುವ ಕನ್ನಡ ಭಾಷೆಯ ಸಾಹಿತ್ಯ ")
( kan_0252 " ನಾರಾಯಣ ಕಸ್ತೂರಿ ರಂಗನಾಥ ಶರ್ಮ ಹುಟ್ಟಿದ್ದು ಕೇರಳ ")
( kan_0253 " ನೆಪ್ಚೂನ್ ಗ್ರಹವು ಮಂದವಾದ ಗ್ರಹ ಉಂಗುರ ವ್ಯವಸ್ಥೆಯನ್ನು ಹೊಂದಿದ್ದು ಈ ಉಂಗುರಗಳ ರಚನೆಯ ವಿವರಗಳು ಇನ್ನೂ ತಿಳಿದುಬಂದಿಲ್ಲ ")
( kan_0254 " ಇದನ್ನು ವಾರದ ಸಹಯೋಗ ಮಾಡಿದರೆ ಹೇಗೆ ")
( kan_0255 " ತಾಯಿ ಪ್ರಸಿದ್ಧ ಸಾಹಿತಿ ಹಾಗು ವಿದ್ವಾಂಸ ")
( kan_0256 " ಕನ್ನಡ ಮತ್ತು ಸಂಸ್ಕೃತ ದಲ್ಲಿ ಎಂ.ಎ. ಪದವಿಯನ್ನು ಪಡೆದರು ")
( kan_0257 " ಕರ್ನಾಟಕ ದ ಶಿವಮೊಗ್ಗ ಜಿಲ್ಲೆಯ ಒಂದು ಊರು ")
( kan_0258 " ಉತ್ತರ ಅಮೇರಿಕ ಮತ್ತು ದಕ್ಷಿಣ ಅಮೇರಿಕ ")
( kan_0259 " ಮಧ್ಯ ಅಮೇರಿಕ ದ ಅತ್ಯಂತ ದಕ್ಷಿಣ ದೇಶ ")
( kan_0260 " ಭಾರತದಲ್ಲಿ ಧಾರ್ಮಿಕ ಪ್ರಾಮುಖ್ಯತೆ ಹೊಂದಿರುವ ಪವಿತ್ರ ಕ್ಷೇತ್ರ ಗಳನ್ನು ಕುರಿತಾದ ಲೇಖನಗಳ ವರ್ಗ ")
( kan_0261 " ಆದರೆ ಕೆಲವರು ಹೆಚ್ ಎಂದೂ ಹೇಳುತ್ತಾರೆ ")
( kan_0262 " ಕನ್ನಡ ಸಾಹಿತ್ಯಕ್ಕೆ ಧಾರವಾಡ ಜಿಲ್ಲೆಯ ಕೊಡುಗೆ ಅಪಾರ ")
( kan_0263 " ಸಾಹಿತಿ ಹೆಸರಿನ ಲೇಖನವನ್ನು ನೀವು ಪ್ರಾರಂಭಿಸಬಹುದು ")
( kan_0264 " ಕೆಳಗೆ ಸಾಹಿತ್ಯಕ್ಕೆ ಸಂಬಂಧಪಟ್ಟ ಪುಟಗಳನ್ನು ಪಟ್ಟಿ ಮಾಡಲಾಗಿದೆ ")
( kan_0265 " ಗೋಕಾಕರು ಇದಕ್ಕೂ ಮೊದಲು ಭಾರತೀಯ ಜ್ಞಾನಪೀಠ ಪ್ರಶಸ್ತಿ ಆಯ್ಕೆ ಸಮಿತಿಯ ಅಧ್ಯಕ್ಷರಾಗಿದ್ದರು ")
