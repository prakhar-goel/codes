( kan_0436 " ಇವರ ಇತ್ತೀಚಿನ ಕನ್ನಡ ಚಲನ ಚಿತ್ರ ಶ್ರೀ ಮಂಜುನಾಥ ದಲ್ಲಿ ನಾಯಕ ನಟನಾಗಿ ಸೌಂದರ್ಯ ಜೊತೆ ನಟಿಸಿದ್ದಾರೆ ")
( kan_0437 " ಕನ್ನಡ ಚಲನಚಿತ್ರ ಉಯ್ಯಾಲೆ ಬಗ್ಗೆ ಮಾಹಿತಿಗೆ ಈ ಲೇಖನ ನೋಡಿ ")
( kan_0438 " ಭೂಮಿ ಯ ಮೂರನೇ ದೊಡ್ಡ ಮಹಾಸಾಗರ ")
( kan_0439 " ಕಡೆಯ ಭಾಗಗಳನ್ನು ಎಡ ಮತ್ತು ಬಲ ಎಂದು ಕರೆಯುತ್ತಾರೆ ")
( kan_0440 " ಕರ್ನಾಟಕ ದ ಒಂದು ಜಿಲ್ಲೆ ಮಂಡ್ಯ ಜಿಲ್ಲೆಯ ರಾಜಧಾನಿ ಮಂಡ್ಯ ನಗರ ")
( kan_0441 " ಇದು ಮುಂಬೈನ ಅತ್ಯಂತ ದುಬಾರಿ ಜಾಗ ")
( kan_0442 " ಅತ್ಯುತ್ತಮ ಹಿನ್ನೆಲೆ ಗಾಯಕ ಎಂದು ಏಳು ಬಾರಿ ರಾಷ್ಟ್ರ ಪ್ರಶಸ್ತಿ ")
( kan_0443 " ಪದವಿ ಪಡೆದು ಬೆಳಗಾವಿ ಯ ಆರ್ ")
( kan_0444 " ಪೂರ್ಣಚಂದ್ರ ತೇಜಸ್ವಿ ಯವರ ಇತ್ತೀಚಿನ ಕೃತಿ ")
( kan_0445 " ದಪ್ಪ ಅಕ್ಷರಗಳು ಹೀಗೆ ಕಾಣಿಸುತ್ತದೆ ದಪ್ಪ ಅಕ್ಷರಗಳು ")
( kan_0446 " ದಾವಣಗೆರೆ ಜಿಲ್ಲೆಯ ತಾಲೂಕು ಗಳ ಬಗ್ಗೆ ಲೇಖನಗಳು ")
( kan_0447 " ಮಾಸ್ತಿ ವೆಂಕಟೇಶ ಅಯ್ಯಂಗಾರ್ ಖ್ಯಾತ ಕನ್ನಡ ಬರಹಗಾರರು ")
( kan_0448 " ಧಾರವಾಡ ದ ಕರ್ನಾಟಕ ಕಾಲೇಜಿನಲ್ಲಿ ಭೌತಶಾಸ್ತ್ರ ದ ಅಧ್ಯಾಪಕರಾಗಿ ಸೇವೆ ಸಲ್ಲಿಸಿ ನಿವೃತ್ತರಾಗಿದ್ದಾರೆ ")
( kan_0449 " ಈಕೆ ಸೀತೆ ಯ ತಂಗಿ ಮತ್ತು ಲಕ್ಷ್ಮಣ ನ ಹೆಂಡತಿ ")
( kan_0450 " ಚದುರಂಗ ಅವರು ಉಯ್ಯಾಲೆ ಕಾದಂಬರಿಯನ್ನು ಬರೆದಿದ್ದಾರೆ ")
( kan_0451 " ಸಂಕ್ರಾಂತಿ ಎಂಬ ಕನ್ನಡ ಚಲನಚಿತ್ರದ ಬಗ್ಗೆ ಮಾಹಿತಿಗೆ ಸಂಕ್ರಾಂತಿ ಚಲನಚಿತ್ರ ನೋಡಿ ")
( kan_0452 " ಮನೆ ಗೆದ್ದ ಮಗ ಕೊನೆಯ ಚಿತ್ರ ")
( kan_0453 " ಕರ್ನಾಟಕ ಏಕೀಕರಣ ಹೆಸರಿನ ಲೇಖನವನ್ನು ನೀವು ಪ್ರಾರಂಭಿಸಬಹುದು ")
( kan_0454 " ಪೈ ರವರಿಗೆ ಸಂಬಂಧಪಟ್ಟ ಮಾಹಿತಿಯನ್ನು ಇಲ್ಲಿ ನೋಡಬಹುದು ")
( kan_0455 " ಮಂಜುನಾಥ ಅವರು ಅಶ್ವಿನಿ ಅವರಿಗೆ ಹತ್ತು ವರ್ಷ ಕೋಚ್ ಆಗಿ ಕೆಲಸಮಾಡಿದ್ದಾರೆ ")
( kan_0456 " ಮಂಗಳೂರು ಕರ್ನಾಟಕದ ಒಂದು ಮುಖ್ಯ ವಾಣಿಜ್ಯ ಕೇಂದ್ರ ")
( kan_0457 " ಪುಣೆಯ ಅತಿ ಜನಪ್ರಿಯ ಕ್ರೀಡೆ ಕ್ರಿಕೆಟ್ ")
( kan_0458 " ಭಾರತ ದ ಖ್ಯಾತ ಟೆನ್ನಿಸ್ ಆಟಗಾರ ")
( kan_0459 " ಜಿ ಪಿ ರಾಜರತ್ನಂ ಖ್ಯಾತ ಕನ್ನಡ ಬರಹಗಾರರು ")
( kan_0460 " ಅದರ ಅನುವಾದ ಮುಗಿದ ಮೇಲೆ ಅದನ್ನು ಮುಖಪುಟಕ್ಕೆ ಹಾಕಬಹುದು ")
( kan_0461 " ಇಲ್ಲಿನ ಪ್ರಮುಖ ಭಾಷೆಗಳು ತುಳು ಕೊಂಕಣಿ ಕನ್ನಡ ಮತ್ತು ಬ್ಯಾರಿ ಭಾಷೆ ")
( kan_0462 " ಶಿವರಾಮ ಕಾರಂತರು ರಚಿಸಿರುವ ಒಂದು ಕಾದಂಬರಿ ")
( kan_0463 " ಸೀತಾರಾಂ ನಿರ್ದೇಶನದ ಇದೇ ಹೆಸರಿನ ಕಿರುತೆರೆ ಧಾರಾವಾಹಿ ಕುರಿತು ಮಾಹಿತಿಗೆ ಈ ಲೇಖನ ನೋಡಿ ")
( kan_0464 " ಮಂಗಳೂರು ಕರ್ನಾಟಕ ದ ದಕ್ಷಿಣ ಕನ್ನಡ ಜಿಲ್ಲೆಯ ಆಡಳಿತ ಕೇಂದ್ರ ")
( kan_0465 " ಕರ್ನಾಟಕ ಏಕೀಕರಣ ಪದವನ್ನು ಬೇರೆ ಲೇಖನಗಳಲ್ಲಿ ಹುಡುಕಿ ")
( kan_0466 " ಕರ್ನಾಟಕ ಸಾಹಿತ್ಯ ಅಕಾಡೆಮಿ ಗೌರವ ಪ್ರಶಸ್ತಿ ಪಡೆದಿದ್ದಾರೆ ")
( kan_0467 " ಉಳಿದ ಖನಿಜಗಳು ಬಹಳ ಕಡಿಮೆ ಪ್ರಮಾಣದಲ್ಲಿ ಸಿಗುತ್ತವೆ ")
( kan_0468 " ಅದೇ ಅವರ ವೃತ್ತಿ ಜೀವನದ ನಾಂದಿ ")
( kan_0469 " ಇವರನ್ನು ಭಾರತದ ಜನತೆ ಮಹಾತ್ಮ ಎಂದು ಕರೆದರು ")
( kan_0470 " ಕನ್ನಡ ವಿಶ್ವವಿದ್ಯಾಲಯ ದ ಜಾನಪದ ವಿಭಾಗದ ಮುಖ್ಯಸ್ಥರು ")
( kan_0471 " ಕನ್ನಡದ ಪ್ರಮುಖ ನಿರ್ದೇಶಕ ನಟ ನಾಗೇಂದ್ರರಾಯರು ಇವರ ತಂದೆ ")
( kan_0472 " ಆರತಿ ವೆಂಕಟೇಶ್ ಇವರು ವೃತ್ತಿಯಲ್ಲಿ ವೈದ್ಯರು ")
( kan_0473 " ಹರಿಹರ ದೇವತೆ ಹಿಂದೂ ಧರ್ಮ ದ ದೇವತೆ ")
( kan_0474 " ನೀವು ಬರೆದ ಮಾಹಿತಿಯನ್ನು ನಾನು ಬಿ ")
( kan_0475 " ಪ್ರಪಂಚದ ಅತ್ಯಂತ ಪುರಾತನ ಗ್ರಂಥಗಳಲ್ಲಿ ವೇದಗಳು ಮುಖ್ಯವಾದುವು ")
( kan_0476 " ಅನೇಕ ಕನ್ನಡಿಗರು ಈ ನಗರದ ವಿವಿಧ ಕ್ಷೇತ್ರಗಳಲ್ಲಿ ಹೆಸರು ಮಾಡಿದ್ದಾರೆ ")
( kan_0477 " ಕೊಲ್ಲಾಪುರ ಅದೇ ಹೆಸರಿನ ಜಿಲ್ಲೆಯ ಕೇಂದ್ರವೂ ಹೌದು ")
